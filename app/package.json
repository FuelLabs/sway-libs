    "format-check": "prettier --check 'src/**/*.{ts,tsx}' --max-warnings=0"
  },
  "eslintConfig": {
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
